        "tolerance = eval(input(\"Masukkan toleransi aproksimasi: \"))\n",
        "\n",
        "akar_newton = NewtonAnalitik(f, fp, tebakan_awal, tolerance)\n",
        "\n",
        "print(f\"Akar dari persamaan f(x) = {formula} adalah x = {akar_newton}\")\n",
        "'''"
